{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "eslint:recommended",
				"plugin:react/recommended"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
				"sourceType": "module"
    },
    "plugins": [
			"react"
		],
    "rules": {
				"react-hooks/exhaustive-deps": "off"
    }
}
